{
    "manifest_version": 2,

    "name": "Overlay",
    "description": "This extension overlays and image on your page",
    "version": "0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs",
        "webNavigation",
        "http://*/*",
        "https://*/*"
    ]
}